# Neon Postgres Configuration
# Copy this file to .env and replace with your actual Neon credentials
# Use the pooled connection URL for Vercel deployment

DATABASE_URL=postgresql://your_user:your_password@your-host-pooler.neon.tech/your_database?sslmode=require

# JWT Configuration
SECRET_KEY=your-secret-key-change-in-production-use-strong-random-key
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_MINUTES=10080
